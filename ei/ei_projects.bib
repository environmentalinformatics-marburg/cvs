@Article{TNauss2009,
  Title                    = {Contributing project with the DFG priority programm 1374 Exploratories for large-scale and long-term functional biodiversity research},
  Author                   = {Thomas Nauss(href)www.umweltinformatik-marburg.de/mitarbeiterinnen-und-mitarbeiter/nauss-thomas/ and Jörg Bendix(href)http://lcrs.geographie.uni-marburg.de/index.php?id=82},
  Chapter                  = {Satellite supported generation of area wide climate and vegetation datasets},
  Editor                   = {Meike Kühnlein},
  Number		   = {1},
  Publisher                = {2009 - 2011 (Deutsche Forschungsgemeinschaft, DFG) },
  Year                     = {2009},
  Note                     = {},
  Comment                  = {<b>Geo-statistical generation of area-wide meteorological datasets</b> <br>Using hourly air temperature and air humidity datasets from 150 grassland stations (50 per exploratory), different geo-statistical interpolation methods (e. g. kriging, splines) have been tested.<br><b>Satellite-based generation of area-wide irradiance datasets </b> A satellite-based correction scheme for the area-wide interpolation of in-situ radiance measurements has been developed.<br><b>Satellite-based generation of area-wide biomass datasets</b> <br>Based on a statistical analysis between in-situ biomass assessments on 150 grass land plots and seasonal time-series of MODIS-derived vegetation indices (NDVI), area-wide biomass dataset, were generated. The normalized difference vegetation index (NDVI) is regarded as an indicator for photosynthetic activity and has been used to link the remotely sensed reflection measurements to the grassland biomass.},
  Keywords                 = {Exploratories},
  Owner                    = {tnauss},
  Timestamp                = {2016.05.04},
  Url                      = {http://www.biodiversity-exploratories.de}
}

@Article{TNauss2007a,
  Title                    = {Central project 1 of the DFG research unit 816 Biodiversity and sustainable management of a megadiverse mountain ecosystem in south Ecuador},
  Author                   = {Thomas Nauss(href)www.umweltinformatik-marburg.de/mitarbeiterinnen-und-mitarbeiter/nauss-thomas/ and Jörg Bendix(href)http://lcrs.geographie.uni-marburg.de/index.php?id=82},
  Chapter                  = {Central data services and SVAT modelling},
  Editor                   = {Dietrich Göttlicher, Maik Dobbermann (working group of J. Bendix)},
  Number		   = {1},
  Publisher                = {2007 - 2010 (Deutsche Forschungsgemeinschaft, DFG) },
  Year                     = {2007},
  Note                     = {},
  Comment                  = {<b>Modelling of data exchange</b> <br>Prior to the implementation of the data warehouse, the expected data exchange between the individual subprojects of the research unit has been modeled using UML. Based on the data supplies from and demands by each subproject, interactions between the subprojects could be visualize.</br><b>Land-cover classifications.</b><br>Area-wide datasets are a fundamental prerequisite for landscape-scale analysis of ecosystem dynamics and land-cover climate interactions. Remote sensing is the only spatially explicit observation method available for environmental sciences and satellite systems offer multi-temporal to quasi-continuous measurements in high to moderate spatial resolution.</br><b>Optical properties of plant functional types</b></br>Vegetation and SVAT models require the parameterization of plant traits to simulate e.g. biomass dynamics or transpiration. If non-individual plant models are used, plants with similar traits have to be grouped and implemented as so-called plant functional types (PFTs). Since area-wide modeling requires a spatially explicit distribution of these PFTs, remote sensing is again the first choice. </br><b>SVAT Modelling</b></br>In order to scale the individual water budget related measurements to the catchment level, the Community Landsurface Model (CLM) - a surface-vegetation-atmosphere-transport model - has been implemented for the Rio San Francisco catchment. },
  Keywords                 = {Ecuador},
  Owner                    = {tnauss},
  Timestamp                = {2016.05.04},
  Url                      = {http://www.umweltinformatik-marburg.de/en/projects/closed/central-data-services-and-svat-modelling/}
}

@Article{TNauss2007b,
  Title                    = {Individual grant},
  Author                   = {Jörg Bendix(href)http://lcrs.geographie.uni-marburg.de/index.php?id=82},
  Chapter                  = {Development of an operationall satellite rainrate retrieval technique for Meteosat-SEVIRI (SORT)},
  Editor                   = {Thomas Nauss},
  Number		   = {1},
  Publisher                = {2007 - 2009 (Deutsche Forschungsgemeinschaft, DFG)},
  Year                     = {2007},
  Note                     = {Boris Thies},
  Comment                  = {Raum-zeitlich hoch variable Niederschläge bilden eine wichtige Komponente innerhalb des hydrologischen Kreislaufs und haben dadurch weitreichende ökologische und sozio-ökonomische Auswirkungen. Im Rahmen des BMBF GLOWA-Danube-Projekts wurde vom Mitantragsteller (Nauß) basierend auf langjährigen Vorarbeiten der Arbeitsgruppe (Retrievaltechniken: CST, ECST, ACT) ein Verfahren entwickelt (Rain Area Delineation Scheme – Daylight, RADS-D), das es erstmals erlaubt, auf der Basis von optischen und mikrophysikalischen Wolkenretrievals die Niederschlagsfläche am Tag aus Daten optischer Sensoren (TERRA/AQUA-MODIS) für die Mittelbreiten akkurat ableiten zu können. Die daran anschließende Dissertation von Herrn Thies hat sich nun zum Ziel gesetzt, (i) die Übertragung der Technik auf das neue europäische geostationäre System (Meteosat Second Generation SEVIRI) zu gewährleisten, (ii) ein vergleichbares Verfahren für Nacht-Daten zu entwickeln (RADS-N), (iii) eine statistische Regenratenzuweisung auf der Basis von Radardaten vorzunehmen und (iv) das Verfahren über einen mehrjährigen Zeitraum zu validieren. Das neu entwickelte operationelle 24-Stunden Verfahren (SEVIRI Operational Rainfall Retrieval Technique SORT) wird für das Gebiet der Bundesrepublik Deutschland entwickelt und validiert.},
  Keywords                 = {Meteosat-SEVIRI},
  Owner                    = {tnauss},
  Timestamp                = {2016.05.04},
  Url                      = {http://www.umweltinformatik-marburg.de/en/projects/closed/development-of-an-operationall-satellite-rainrate-retrieval-technique-for-meteosat-seviri-sort/}
}

