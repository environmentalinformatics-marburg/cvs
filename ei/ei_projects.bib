@InBook{TNaussIDESSA,
  Title                    = {Project within the BMBF project: An integrative decision support system for sustainable rangeland management in southern African savannas (IDESSA)},
  Author                   = {Thomas Nauss,www.umweltinformatik-marburg.de/mitarbeiterinnen-und-mitarbeiter/nauss-thomas/},
  Chapter                  = {EcoDynamics@BMBF},
  Editor                   = {Kerstin Wiegand and Katrin M. Meyer (Georg-August-Universität Göttingen) and Bernhard Seeger (Philipps-Universität Marburg),https://www.uni-marburg.de/fb12/dbs/team/?language_sync=1 and David Ward (University of KwaZulu-Natal) and  Klaus Kellner (North-West University) },
  Publisher                = {DFG},
  Year                     = {since 2014 (Bundesministerium für Forschung und Bildung, BMBF) },
  Note                     = {Hanna Meyer,http://www.umweltinformatik-marburg.de/mitarbeiterinnen-und-mitarbeiter/meyer-hanna/},
  Comment                  = {Land degradation is becoming a serious problem in southern Africa, especially in the geographically extensive savannas, where trees and grasses share dominance. Land degradation in savannas is manifested largely as woody plant encroachment, which suppresses the grazing capacity of these extensive rangelands. Potential drivers of savanna dynamics are rainfall, livestock grazing, fire and use of herbicides. Little is known about how ecosystem stability of the savanna system will be challenged by altered precipitation patterns and increasing CO2 and temperature levels in the context of global climate.},
  Keywords                 = {IDESSA},
  Owner                    = {tnauss},
  Timestamp                = {2016.05.12},
  Url                      = {http://idessa.org}
}

