@Article{TNauss2011b,
  Title                    = {Core project of the DFG priority programm 1374: Exploratories for large-scale and long-term functional biodiversity research},
  Author		   = {Thomas Nauss(href)http://www.umweltinformatik-marburg.de/mitarbeiterinnen-und-mitarbeiter/nauss-thomas/},
  Chapter                  = {Instrumentation},
  Editor                   = {Markus Fischer},
  Number		   = {2},
  Publisher                = {Since 2011 (Deutsche Forschungsgemeinschaft, DFG) },
  Year                     = {2011},
  Note			   = {Falk Hänsel(href)http://www.umweltinformatik-marburg.de/mitarbeiterinnen-und-mitarbeiter/haensel-falk/},
  Comment                  = {At each of the three biodiversity exploratories (biosphere reserve Schorfheide-Chorin, national park Hainich, biosphere reserve Schwäbische Alb, all Germany), 100 climate stations have been installed on grassland research plots. <br>At each of the sites, air temperature and humidity (+2 m), soil surface temperature (+10 cm), soil moisture (-10 cm) and soil temperature (-5 cm, -10 cm, -20 cm, -50 cm) are recorded in 30 minute intervals. On 21 selected stations, the four component radiation balance, photosynthetic active radiation, wind speed/direction as well as rainfall are additionally recorded. <br>The stations provide baseline meteorological information for biodiversity analysis of the 41 research projects currently funded by the priority program. },
  Keywords                 = {Exploratories},
  Owner                    = {tnauss},
  Timestamp                = {2016.05.12},
  Url                      = {http://www.biodiversity-exploratories.de}
}

